{
  "name": "holly_react",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://lukemcdonald.github.io/holly-react",
  "scripts": {
    "build": "npm run build:css && build:js",
    "build:css": "postcss styles/tailwind.css -o ./src/assets/style.css",
    "build:js": "react-scripts build",
    "deploy": "npm run build && gh-pages -d build",
    "start": "concurrently \"npm run start:css\" \"npm run start:js\"",
    "start:css": "postcss styles/tailwind.css -o ./src/assets/style.css --watch",
    "start:js": "react-scripts start"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.2",
    "clsx": "^1.1.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-custom-properties": "^12.1.3",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "scrollreveal": "^4.0.9",
    "tailwindcss": "^3.0.16"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-wesbos": "lukemcdonald/eslint-config-wesbos",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "react-scripts": "5.0.0",
    "watch": "^1.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
